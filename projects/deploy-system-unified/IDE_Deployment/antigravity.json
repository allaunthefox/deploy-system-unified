{
  "analysis": {
    "engine": "python",
    "pythonPath": "/usr/bin/python3",
    "targetDirectories": [
      "roles",
      "branch_templates"
    ],
    "visualization": {
      "port": 8080,
      "theme": "dark",
      "charts": [
        "dependency_graph",
        "task_execution_time"
      ]
    }
  },
  "deployment": {
    "provider": "contabo",
    "region": "auto",
    "optimization_profile": "vps_hardened"
  },
  "security": {
    "scan_on_save": true,
    "scanners": [
      "trivy",
      "checkov",
      "ansible-lint"
    ]
  }
}
